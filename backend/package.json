{
  "name": "alisandbox",
  "version": "1.0.0",
  "description": "Restaurant promo video generator",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --loader ts-node/esm index.ts",
    "build": "tsc",
    "generate": "node --loader ts-node/esm index.ts ./sample_input.json ./output",
    "server": "node --loader ts-node/esm server.ts",
    "frontend": "cd ../frontend && npm run dev -- --port 3001",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ffmpeg-static": "^5.2.0",
    "multer": "^2.0.0",
    "openai": "^4.103.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^20.17.50",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}

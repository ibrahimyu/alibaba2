# Alibaba Cloud API Keys
ALIBABA_API_KEY=your_alibaba_cloud_api_key_here
ALIBABA_OSS_ACCESS_KEY_ID=your_oss_access_key_id_here
ALIBABA_OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret_here
ALIBABA_OSS_ENDPOINT=your_oss_endpoint_here
ALIBABA_OSS_BUCKET=your_oss_bucket_name_here
ALIBABA_OSS_URL_PREFIX=https://your-bucket.oss-your-region.aliyuncs.com

# OpenAI API for narration
OPENAI_API_KEY=your_openai_api_key_here

# YuE Music Generation Configuration
# Path to the YuE repository (required)
YUE_REPO_PATH=/path/to/YuE
# Path to the checkpoint directory (optional)
YUE_CHECKPOINT_DIR=/path/to/YuE/checkpoints
